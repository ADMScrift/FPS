local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()


local Window = OrionLib:MakeWindow({Name = "ðŸŸ¡FPS Boosterâšª", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


local FPSTab = Window:MakeTab({
	Name = "ðŸ‘»FPS BoosterðŸ‘»",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

OrionLib:MakeNotification({
	Name = "Bem vindo!",
	Content = "Lista ugc Script",
	Image = "rbxassetid://4483345998",
	Time = 5
})

FPSTab:AddButton({
    Name = "Remover Textura",
    Callback = function()  ----script logo abaixo:arrow_down::arrow_down::arrow_down:-----
        for i,v in next, (workspace:GetDescendants()) do
            if v:IsA("Part") then v.Material = Enum.Material.SmoothPlastic
            end
            end
    end    
})

FPSTab:AddButton({
    Name = "Remover textura V2",
    Callback = function()  ----script logo abaixo:arrow_down::arrow_down::arrow_down:-----
		loadstring(game:HttpGet('https://pastebin.com/raw/Qn1cfEmN'))()

 	end    
 })




FPSTab:AddButton({
    Name = "Remover FOG",
    Callback = function()  ----script logo abaixo:arrow_down::arrow_down::arrow_down:-----
		repeat task.wait() until game:IsLoaded()
    wait(5) -- extra load to ensure the script is working
    for i,v in pairs(game.Lighting:GetChildren()) do
	if v:IsA("") or v:IsA("Sky") or v:IsA("BlurEffect") or v:IsA("BloomEffect") or v:IsA("SunRaysEffect") then
	v:Destroy()
	end
	end

    end    
})

 FPSTab:AddButton({
    Name = "Mostrar Fps",
    Callback = function()  ----script logo abaixo:arrow_down::arrow_down::arrow_down:-----
		loadstring(game:HttpGet('https://raw.githubusercontent.com/1201for/littlegui/main/FPS-Counter'))()

 	end    
 })
